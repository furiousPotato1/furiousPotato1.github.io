{
  "name": "demo-projects",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:demo-projects": "node dist/demo-projects/server/server.mjs",
		"start-network": "ng serve --host 0.0.0.0 --port 4200 --ssl true --ssl-key ssl/key.pem --ssl-cert ssl/cert.pem",
  	"generate-ssl": "mkdir ssl && openssl req -x509 -newkey rsa:2048 -nodes -keyout ssl\\key.pem -out ssl\\cert.pem -days 365 -subj \"/CN=192.168.0.213\""
  },
  "private": true,
